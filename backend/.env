# Server Configuration
PORT=3000
NODE_ENV=development

# Firebase Admin Configuration
FIREBASE_PROJECT_ID=sporelinkapp
FIREBASE_CLIENT_EMAIL=<your-service-account-email>
FIREBASE_PRIVATE_KEY=<your-private-key>
FIREBASE_STORAGE_BUCKET=sporelinkapp.appspot.com

# CORS Configuration
ALLOWED_ORIGINS=http://localhost:3000,https://sporelinkapp.web.app

# Security
JWT_SECRET=your-jwt-secret
JWT_EXPIRES_IN=7d

# Logging
LOG_LEVEL=debug 